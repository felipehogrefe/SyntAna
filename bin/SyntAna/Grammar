DECMETODO = CABMETODO CORPOMETODO
CABMETODO = MODIFICADOR TIPOMETODO X
LISTPARAMFORMAL = PARAMFORMAIS LISTPARAMFORMAL1
LISTPARAMFORMAL1 = ',' PARAMFORMAIS LISTPARAMFORMAL
PARAMFORMAIS = TIPO X 
CORPOMETODO = BLOCO 
		| ';'

---------
X = 'id' A
A = '[' B 
		| '[' B
B = ']' 
		| LISTPARAMFORMAL ']'
---------
decconstru

DECCONSTRU = NOMESIMPLES '[' LISTPARAMFORMAL ']'

Produções referentes a BLOCO e DECLARACAO

BLOCO = '{' DECBLOCO '}'
DECBLOCO = BLOCODEC DECB1
DECB1 = DECBLOCO DECB1 
		| null
BLOCODEC = DECCAMPO 
		| DECLARACAO
DECLARACAO = DECSEMSUBDECDIR 
		| IFTHENDEC 
        | IFTHELESEDEC 
        | DECWHILE 
        | DECFOR 
DECSEMSUBDECDIR = BLOCO 
		| ';' 
		| EXPRESSAODEC 
        | DECRETURN 
        | DECBREAK
EXPRESSAODEC = ATRIBUICAO 
		| EXPRINCRPRE 
		| EXPRDECREPRE 
		| EXPRINCRPOS 
        | EXPRDECREPOS 
        | CHAMADAMETODO 
        | EXPRCRIAINSTANCLASSE 
DECNSI = DECSEMSUBDECDIR 
		| ITEDECNSI 
		| WDECNSI 
		| FDECNSI 
IFTHENDEC = 'if' '[' EXPRATR ']'  DECLARACAO 
IFTHELESEDEC = 'if' '[' EXPRATR ']' DECNSI  'else' DECLARACAO
ITEDECNSI = 'if' '[' EXPRATR ']' DECNSI 'else' DECNSI 
DECWHILE = 'while' '[' EXPRATR ']' DECLARACAO
WDECNSI = 'while' '[' EXPRATR ']' DECNSI 
DECFOR = 'for' '[' FORINIT ';' LIMITESUP ';' PASSO ']' DECLARACAO
FDECNSI = 'for' '[' FORINIT ';' LIMITESUP ';' PASSO ']' DECNSI 
*FORINIT = TIPOPRIMITIVONUM NOME '=' EXPRATR
*LIMITESUP = CTN 
		| NOME
*PASSO = CTN 
		| NOME
LISTADECEXPR = EXPRESSAODEC
		| LISTADECEXPR ';' EXPRESSAODEC 
*DECRETURN = 'return' EXPRATR ';' 
*DECBREAK = 'break' ';'

Produções referente a EXPRESSAO

*EXPRCRIAINSTANCLASSE = 'new' NOME '[' LISTAARG ']'
*LISTAARG = EXPRATR LISTA1
*LISTA1 = ',' LISTAARG 
		| null 
*EXPRCRIAARRAY= 'array' '[' TIPO ',' CTN ']'
CHAMADAMETODO = NOME '[' LISTAARG ']'
*PRIMARIO = PRISEMNOVOARRAY 
		| EXPRCRIAARRAY
*PRISEMNOVOARRAY =  'this' 
        | '[' EXPRATR ']'
        | LITERAL
        | EXPRCRIAINSTANCLASSE
    	| CHAMADAMETODO 
*ATRIBUICAO = NOME NOME1 '=' EXPRATR 
*NOME1 = '[' EXPRATR ']'
		| null
*EXPRATR = EXPRCOND 
		| ATRIBUICAO
*EXPRCOND = EXPRORCOND EXPRCOND1
*EXPRCOND1 = '?' EXPRATR ':' EXPRCOND 
		| null
*EXPRORCOND = EXPRECOND EXPROR1
*EXPROR1 = '||' EXPRECOND EXPROR1 
		| null 
*EXPRECOND = EXPRIOR EXPREC1 
*EXPREC1 = '&&' EXPRIOR EXPREC1 
		| null
*EXPRIOR = EXPREOR EXPRI1
*EXPRI1 = '|' EXPRIOR EXPRI1 
		| null
*EXPREOR = EXPRE EXPREOR1
*EXPREOR1 = '^' EXPREOR EXPREOR1 
		| null
*EXPRE = EXPREQUA EXPRE1
*EXPRE1 = '&' EXPREQUA EXPRE1 
		| null
*EXPREQUA = EXPRREL EXPREQ1
*EXPREQ1 = '==' EXPREQUA EXPREQ1 
	    | '!=' EXPREQUA EXPREQ1 
		| null
*EXPRREL = EXPRADD EXPRREL1
*EXPRREL1 =  '<' EXPRADD EXPRREL1 
		| '>' EXPRADD EXPRREL1 
		| '<=' EXPRADD EXPRREL1  
		| '>=' EXPRADD EXPRREL1 
		| 'instanceof' TIPOREF EXPRREL1
		| null
*EXPRADD = EXPRMULT EXPRADD1
*EXPRADD1 = '+' EXPRMULT EXPRADD1
        |  '-' EXPRMULT EXPRADD1 
        | null
*EXPRMULT = EXPRUNARIA EXPRMULT1
*EXPRMULT1 = '*' EXPRUNARIA EXPRMULT1 
		| '/' EXPRUNARIA EXPRMULT1 
		| '%' EXPRUNARIA EXPRMULT1
        | null
*EXPUNNMM = EXPRPOSF 
		| '~' EXPRUNARIA 
		| '!' EXPRUNARIA
*EXPRDECREPRE = '--' EXPRUNARIA
*EXPRINCRPRE =  '++' EXPRUNARIA
*EXPRUNARIA = '--' EXPRUNARIA
		| '++' EXPRUNARIA
        | '+' EXPRUNARIA
        | '-' EXPRUNARIA 
		| '~' EXPRUNARIA 
		| '!' EXPRUNARIA
        | EXPRPOSF 
*EXPRDECREPOS = EXPRPOSF '--'
*EXPRINCRPOS = EXPRPOSF '++'
EXPRPOSF = PRIMARIO 
		| NOME 
		| EXPRINCRPOS 
        | EXPRDECREPOS